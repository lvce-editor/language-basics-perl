{
  "id": "builtin.language-basics-perl",
  "name": "Language Basics Perl",
  "description": "Provides syntax highlighting and bracket matching in Perl files.",
  "languages": [
    {
      "id": "perl",
      "extensions": [".pl", ".pm", ".pod", ".t", ".pL", ".psgi"],
      "tokenize": "src/tokenizePerl.js",
      "firstLine": "^#!.*\\bperl\\b"
    }
  ]
}
